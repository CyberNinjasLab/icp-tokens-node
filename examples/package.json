  "devDependencies": {
    "@types/node": "^18.11.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "@icptokens/nns-integration": "workspace:*",
    "@icptokens/dex-integration": "workspace:*",
    "@icptokens/bob-integration": "workspace:*",
    "@icptokens/token-toolkit": "workspace:*"
  }
